{
// Run button configuration
  "commands": {
        "Git - version": "git --version",
        "Git - Reconnect": "eval $(ssh-agent -s) \n ssh-add ~/.ssh/id_rsa_mayberry\n",
        "MongoDb version": "mongo --version",
        "recColl - Connect": "mongo recColl \n show collections\n",
        "recColl - Show Counts": "mongo recColl \n show collections \n db.company.count() \n db.genre.count() \n db.vinyl.count()",
        "Export - Vinyl": "sudo mongoexport --db recColl -c vinyl --out db/export/vinyl.json",
        "Export - Genre": "sudo mongoexport --db recColl -c genre --out db/export/genre.json",
        "Export - Company": "sudo mongoexport --db recColl -c company --out db/export/company.json"
  },

// Preview button configuration
  "preview": {
        "Box URL": "http://{{domain3000}}/",
        "Box URL SSL": "https://{{domain3000}}/",
        "Project Index (static)": "https://{{domain}}/{{index}}",
        "Current File (static)": "https://{{domain}}/{{filepath}}"
  }
}